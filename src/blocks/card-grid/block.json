{
    "apiVersion": 2,
    "name": "evergreen/card-grid",
    "version": "1.0.0",
    "title": "Card Grid",
    "category": "design",
    "icon": "grid-view",
    "description": "A grid container for card blocks",
    "supports": {
        "html": false,
        "anchor": true,
        "align": ["wide", "full"]
    },
    "attributes": {
        "columns": {
            "type": "number",
            "default": 3
        },
        "gap": {
            "type": "number",
            "default": 24
        }
    },
    "textdomain": "evergreen",
    "editorScript": "file:./index.js",
    "editorStyle": "file:./editor.css",
    "style": "file:./style.css"
} 